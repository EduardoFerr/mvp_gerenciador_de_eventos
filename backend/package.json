    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.15",
    "zod": "^3.23.8",
    "ts-node": "^10.9.2",
    "@types/bcryptjs": "^2.4.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.9",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
